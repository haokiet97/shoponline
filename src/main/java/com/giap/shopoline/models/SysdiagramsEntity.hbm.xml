<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.giap.shopoline.models.SysdiagramsEntity" table="sysdiagrams" schema="dbo" catalog="QuanLiDT">
        <id name="diagramId">
            <column name="diagram_id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="sysname"/>
        </property>
        <property name="principalId">
            <column name="principal_id" sql-type="int"/>
        </property>
        <property name="version">
            <column name="version" sql-type="int" not-null="true"/>
        </property>
        <property name="definition">
            <column name="definition" sql-type="varbinary(max)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>