<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.giap.shopoline.models.TblNhasanxuatEntity" table="tbl_nhasanxuat" schema="dbo" catalog="QuanLiDT">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="ten">
            <column name="ten" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <set name="tblSanphamsById" inverse="true">
            <key>
                <column name="id_nhasanxuat" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.giap.shopoline.models.TblSanphamEntity"/>
        </set>
    </class>
</hibernate-mapping>