<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.giap.shopoline.models.TblTaikhoanEntity" table="tbl_taikhoan" schema="dbo" catalog="QuanLiDT">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="idQuyen">
            <column name="id_quyen" sql-type="int" not-null="true"/>
        </property>
        <property name="tentaikhoan">
            <column name="tentaikhoan" sql-type="nvarchar(50)" length="50" not-null="true"/>
        </property>
        <property name="matkhau">
            <column name="matkhau" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="idNhanvien">
            <column name="id_nhanvien" sql-type="int" not-null="true"/>
        </property>
        <property name="trangthai">
            <column name="trangthai" sql-type="bit" not-null="true"/>
        </property>
        <many-to-one name="tblQuyenByIdQuyen" class="com.giap.shopoline.models.TblQuyenEntity">
            <column name="id_quyen" not-null="true"/>
        </many-to-one>
        <many-to-one name="tblNhanvienByIdNhanvien" class="com.giap.shopoline.models.TblNhanvienEntity">
            <column name="id_nhanvien" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>