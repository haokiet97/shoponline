<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.giap.shopoline.models.TblAlbumAnhEntity" table="tbl_album_anh" schema="dbo" catalog="QuanLiDT">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="anh1">
            <column name="anh1" sql-type="nvarchar(250)" length="250" not-null="true"/>
        </property>
        <property name="anh2">
            <column name="anh2" sql-type="nvarchar(250)" length="250" not-null="true"/>
        </property>
        <property name="anh3">
            <column name="anh3" sql-type="nvarchar(250)" length="250" not-null="true"/>
        </property>
        <property name="anh4">
            <column name="anh4" sql-type="nvarchar(250)" length="250" not-null="true"/>
        </property>
        <property name="temp">
            <column name="temp" sql-type="int" not-null="true"/>
        </property>
        <property name="idSanpham">
            <column name="id_sanpham" sql-type="int" not-null="true"/>
        </property>
        <many-to-one name="tblSanphamByIdSanpham" class="com.giap.shopoline.models.TblSanphamEntity">
            <column name="id_sanpham" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>