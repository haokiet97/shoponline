<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.giap.shopoline.models.TblSanphamEntity" table="tbl_sanpham" schema="dbo" catalog="QuanLiDT">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="idDanhmuc">
            <column name="id_danhmuc" sql-type="int" not-null="true"/>
        </property>
        <property name="ten">
            <column name="ten" sql-type="nvarchar(150)" length="150" not-null="true"/>
        </property>
        <property name="gia">
            <column name="gia" sql-type="decimal(12)" precision="12" not-null="true"/>
        </property>
        <property name="soluong">
            <column name="soluong" sql-type="int" not-null="true"/>
        </property>
        <property name="trongluong">
            <column name="trongluong" sql-type="nchar(10)" length="10" not-null="true"/>
        </property>
        <property name="rom">
            <column name="ROM" sql-type="nchar(10)" length="10" not-null="true"/>
        </property>
        <property name="ram">
            <column name="RAM" sql-type="nchar(10)" length="10" not-null="true"/>
        </property>
        <property name="thenho">
            <column name="thenho" sql-type="nvarchar(50)" length="50" not-null="true"/>
        </property>
        <property name="cameraTruoc">
            <column name="camera_truoc" sql-type="nchar(10)" length="10" not-null="true"/>
        </property>
        <property name="cameraSau">
            <column name="camera_sau" sql-type="nchar(10)" length="10" not-null="true"/>
        </property>
        <property name="pin">
            <column name="pin" sql-type="int" not-null="true"/>
        </property>
        <property name="baohanh">
            <column name="baohanh" sql-type="nchar(50)" length="50" not-null="true"/>
        </property>
        <property name="bluetooth">
            <column name="bluetooth" sql-type="int" not-null="true"/>
        </property>
        <property name="idNhasanxuat">
            <column name="id_nhasanxuat" sql-type="int" not-null="true"/>
        </property>
        <property name="cpu">
            <column name="CPU" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="manhinh">
            <column name="manhinh" sql-type="nvarchar(50)" length="50" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int" not-null="true"/>
        </property>
        <many-to-one name="tblDanhmucByIdDanhmuc" class="com.giap.shopoline.models.TblDanhmucEntity">
            <column name="id_danhmuc" not-null="true"/>
        </many-to-one>
        <many-to-one name="tblNhasanxuatByIdNhasanxuat" class="com.giap.shopoline.models.TblNhasanxuatEntity">
            <column name="id_nhasanxuat" not-null="true"/>
        </many-to-one>
        <set name="tblAlbumAnhsById" inverse="true">
            <key>
                <column name="id_sanpham" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.giap.shopoline.models.TblAlbumAnhEntity"/>
        </set>
        <set name="tblAnhsById" inverse="true">
            <key>
                <column name="id_sanpham" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.giap.shopoline.models.TblAnhEntity"/>
        </set>
        <set name="tblBinhluansById" inverse="true">
            <key>
                <column name="id_sanpham" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.giap.shopoline.models.TblBinhluanEntity"/>
        </set>
        <set name="tblChitietdonhangsById" inverse="true">
            <key>
                <column name="id_sanpham"/>
            </key>
            <one-to-many not-found="ignore" class="com.giap.shopoline.models.TblChitietdonhangEntity"/>
        </set>
    </class>
</hibernate-mapping>